{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "Node 20",
  "_version": "20.1.0",
  "compilerOptions": {
    "lib": [
      "es2023"
    ],
    "module": "NodeNext",
    "target": "ES2020",
    "strict": true,
    "sourceMap": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "moduleResolution": "NodeNext",
    "outDir": "dist",
    "rootDir": "./"
  },
  "baseUrl": ".",
  "paths": {
    "@/videos/*": [
      "./src/videos/*"
    ],
    "@/lib/*": [
      "./src/lib/*"
    ]
  },
  "include": ["src/**/*"]
}